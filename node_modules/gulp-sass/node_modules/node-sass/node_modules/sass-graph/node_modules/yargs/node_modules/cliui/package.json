{
  "name": "cliui",
  "version": "3.1.0",
  "description": "easily create complex multi-column command-line-interfaces",
  "main": "index.js",
  "scripts": {
    "test": "standard && nyc mocha",
    "coverage": "nyc --reporter=text-lcov mocha | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/bcoe/cliui.git"
  },
  "config": {
